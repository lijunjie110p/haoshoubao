.container {
  padding: 0 20rpx;
  background: #FFFFFF;
  width: 710rpx;
}
.main {
  width: 750rpx;
  position: relative;
}
.font-blod {
  font-weight: bold;
}
.main-bg {
  background: #FFBA37;
}
.main-color {
  color: #FFBA37;
}
.white-color {
  color: #FFF;
}
.white-bg {
  background-color: #FFF;
}
.gray-bg {
  background: #F2F2F2;
}
.padding-width-base {
  padding: 0 30rpx;
}
.sub-btn {
  background: 'linear-gradient(270deg,rgba(255,186,0,1),rgba(255,207,0,1))';
  height: '90rpx';
  color: '#FFF';
  border: 'none';
  font-size: '36rpx';
}
page {
  background: #F2F2F2;
}
.main {
  background-image: url("https://lanmao-res.oss-cn-qingdao.aliyuncs.com/static/style/2020-08-04/5f2947663396b.png");
  background-size: 100% 216rpx;
  background-repeat: no-repeat;
}
.headpic {
  height: 200rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.headpic .title {
  font-size: 50rpx;
  color: #DEBB91;
  font-weight: 400;
}
.form {
  padding: 30rpx;
  border-radius: 20rpx 20rpx 0 0;
  overflow: hidden;
  background: #FFF;
  z-index: 999;
}
.upload {
  padding: 30rpx;
  background: #FFF;
}
.upload .u-upload {
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.upload .slot-btn {
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  margin: 40rpx auto;
  height: 330rpx;
  width: 600rpx;
  border: 1px solid #dcdcdc;
  overflow: hidden;
}
.upload .slot-btn .text {
  font-size: 30rpx;
  color: #FFBA00;
  margin-top: 10rpx;
}
.foot {
  padding: 70rpx 30rpx;
