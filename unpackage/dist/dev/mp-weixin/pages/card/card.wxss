.container {
  padding: 0 20rpx;
  background: #FFFFFF;
  width: 710rpx;
}
.main {
  width: 750rpx;
  position: relative;
}
.font-blod {
  font-weight: bold;
}
.main-bg {
  background: #FFBA37;
}
.main-color {
  color: #FFBA37;
}
.white-color {
  color: #FFF;
}
.white-bg {
  background-color: #FFF;
}
.gray-bg {
  background: #F2F2F2;
}
.padding-width-base {
  padding: 0 30rpx;
}
.sub-btn {
  background: 'linear-gradient(270deg,rgba(255,186,0,1),rgba(255,207,0,1))';
  height: '90rpx';
  color: '#FFF';
  border: 'none';
  font-size: '36rpx';
}
.tabs {
  background: #F6F6F6;
  padding: 0 30rpx;
}
.nocard {
  margin: 20rpx 30rpx;
  box-shadow: 8px 2px 12px 0px rgba(189, 189, 189, 0.35), -8px 2px 12px 0px rgba(189, 189, 189, 0.35);
  border-radius: 20rpx;
  background: #FFF;
  height: 280rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.nocard .addbtn {
  width: 380rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  border-radius: 50rpx;
  background: -webkit-linear-gradient(right, #ffba00, #ffcf00);
  background: linear-gradient(270deg, #ffba00, #ffcf00);
  color: #FFF;
}
.hascard {
  padding: 0 30rpx 50rpx 30rpx;
  background: #FFF;
}
.hascard .card {
  margin: 30rpx 0;
  padding: 40rpx 50rpx;
  box-shadow: 8px 2px 12px 0px rgba(189, 189, 189, 0.35), -8px 2px 12px 0px rgba(189, 189, 189, 0.35);
  border-radius: 20rpx;
}
.botttom-addbtn {
  width: 600rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  border-radius: 50rpx;
  margin: 50rpx auto;
  background: -webkit-linear-gradient(right, #ffba00, #ffcf00);
  background: linear-gradient(270deg, #ffba00, #ffcf00);
  color: #FFF;
}
.cash-card {
  margin: 30rpx 0;
  padding: 30rpx 40rpx;
  border-radius: 20rpx;
  color: #FFF;
  position: relative;
}
.cash-card .icon {
  background: #FFFFFF;
  border-radius: 50rpx;
  padding: 5rpx;
}
.cash-card .bgimg {
  position: absolute;
  right: 50rpx;
  width: 150rpx;
  top: 30rpx;
  bottom: 30rpx;
  background: transparent;
}
.cash-card .bgimg .u-image {
  background: transparent;
}

